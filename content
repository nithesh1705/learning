%Y-%m-%dT%H:%M:%S.%fZ",       # ISO 8601 with microseconds and 'Z'
    "%Y-%m-%dT%H:%M:%SZ",          # ISO 8601 without microseconds, with 'Z'
    "%Y-%m-%dT%H:%M:%S.%f+00:00",  # ISO 8601 with offset (+00:00)
    "%Y-%m-%dT%H:%M:%S+00:00",     # ISO 8601 without microseconds, offset
    "%Y-%m-%dT%H:%M:%S.%f",        # ISO 8601 UTC without Z or offset
    "%Y-%m-%dT%H:%M:%S",           # Basic datetime, no microseconds or offset
    "%Y-%m-%d %H:%M:%S",           # Space-separated datetime
    "%Y-%m-%d %H:%M:%S.%f"
